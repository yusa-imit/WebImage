{
  "name": "webimage",
  "productName": "WebImage",
  "version": "1.0.0",
  "description": "Convert media files into web-friendly formats.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "esimi",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "nozomi.la_crawler"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-react": "^7.14.5",
    "@electron-forge/cli": "^6.0.0-beta.56",
    "@electron-forge/maker-deb": "^6.0.0-beta.56",
    "@electron-forge/maker-rpm": "^6.0.0-beta.56",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.56",
    "@electron-forge/maker-zip": "^6.0.0-beta.56",
    "@electron-forge/plugin-webpack": "6.0.0-beta.56",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.0",
    "electron": "13.1.4",
    "json-loader": "^0.5.7",
    "node-loader": "^2.0.0",
    "py-loader": "^0.3.1",
    "redux-logger": "^3.0.6",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "dotenv": "^10.0.0",
    "electron-acrylic-window": "^0.5.5",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "ffmpeg-static": "^4.4.0",
    "ffmpeg-static-electron": "^2.0.3",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "image-conversion": "^2.1.1",
    "image-size": "^1.0.0",
    "particles-bg": "^2.5.5",
    "rc-progress": "^3.1.4",
    "react": "^17.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-custom-scrollbars-2": "^4.4.0",
    "react-delay-input": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.5",
    "react-select": "^4.3.1",
    "react-video-js-player": "^1.1.1",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "sharp": "^0.29.1",
    "use-state-with-callback": "^2.0.3",
    "webpack": "^5.41.0"
  }
}
